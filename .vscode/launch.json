{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/BackgroundRemover/bin/Debug/net8.0/BackgroundRemover.dll",
            "args": [
                // "-i", "../images/me.jpg",
                "-f", "../images"
            ],
            "cwd": "${workspaceFolder}/BackgroundRemover",
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": false
            },
        }
    ]
}
